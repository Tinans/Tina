/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.btn {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0;
  margin-top: 5px;
  width: 100%;
  height: 50px;
  font-size: 18px;
  background-color: #f8f8f8;
}

button {
  width: 500rpx!important;
}

.data {
  padding-top: 30rpx;
}

.data .text {
  font-size: 20px;
  font-weight: 600;
}
